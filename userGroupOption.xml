<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/userGroupOption.xsd">
	<import>
		<categories>
			<category name="admin.show">
				<parent>admin.application</parent>
			</category>
			<category name="mod.show">
				<parent>mod</parent>
			</category>
			<category name="user.show">
				<parent>user</parent>
			</category>
		</categories>
		
		<options>
			<!-- admin -->
			<option name="admin.show.canManageCategory">
				<categoryname>admin.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="admin.show.canManageEntryOption">
				<categoryname>admin.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
				<usersonly>1</usersonly>
			</option>
			
			<!-- mod -->
			<!-- mod entry -->
			<option name="mod.show.canEditEntry">
				<categoryname>mod.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="mod.show.canDeleteEntry">
				<categoryname>mod.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="mod.show.canModerateEntry">
				<categoryname>mod.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="mod.show.canViewDeletedEntry">
				<categoryname>mod.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="mod.show.canRestoreEntry">
				<categoryname>mod.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="mod.show.canDeleteEntryCompletely">
				<categoryname>mod.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			
			<!-- mod comment -->
			<option name="mod.show.canEditComment">
				<categoryname>mod.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="mod.show.canDeleteComment">
				<categoryname>mod.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="mod.show.canModerateComment">
				<categoryname>mod.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			
			<!-- user -->
			<!-- user entry -->
			<option name="user.show.canViewEntry">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.show.canAddEntry">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>user.show.canAddEntryWithoutModeration</enableoptions>
				<usersonly>1</usersonly>
			</option>
			<option name="user.show.canAddEntryWithoutModeration">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="user.show.canEditEntry">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="user.show.canEditEntryOfOthers">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="user.show.canDeleteEntry">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="user.show.canDisableCommentFunction">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="user.show.maxTextLength">
				<categoryname>user.show</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>50000</defaultvalue>
				<usersonly>1</usersonly>
			</option>
			
			<option name="user.show.maxCategories">
				<categoryname>user.show</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>100</defaultvalue>
				<minvalue>2</minvalue>
				<usersonly>1</usersonly>
			</option>
			
			<option name="user.show.maxAttachmentSize">
				<categoryname>user.show</categoryname>
				<optiontype>fileSize</optiontype>
				<defaultvalue>2000000</defaultvalue>
				<options>module_attachment</options>
				<minvalue>10000</minvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="user.show.allowedAttachmentExtensions">
				<categoryname>user.show</categoryname>
				<optiontype>textarea</optiontype>
				<defaultvalue>gif
jpg
jpeg
png</defaultvalue>
				<options>module_attachment</options>
				<wildcard>*</wildcard>
				<usersonly>1</usersonly>
			</option>
			<option name="user.show.maxAttachmentCount">
				<categoryname>user.show</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>10</defaultvalue>
				<options>module_attachment</options>
				<minvalue>1</minvalue>
				<maxvalue>100</maxvalue>
				<usersonly>1</usersonly>
			</option>
			
			<!-- user comment -->
			<option name="user.show.canAddComment">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<userdefaultvalue>1</userdefaultvalue>
				<enableoptions>user.show.canAddCommentWithoutModeration</enableoptions>
				<excludedInTinyBuild>1</excludedInTinyBuild>
			</option>
			<option name="user.show.canAddCommentWithoutModeration">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<excludedInTinyBuild>1</excludedInTinyBuild>
			</option>
			<option name="user.show.canEditComment">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="user.show.canDeleteComment">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<usersonly>1</usersonly>
			</option>
			
			<option name="user.show.canViewContact">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			
			<option name="user.show.canSetTags">
				<categoryname>user.show</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<options>module_tagging</options>
			</option>
		</options>
	</import>
</data>